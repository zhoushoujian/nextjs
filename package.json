{
  "name": "nextjs-example",
  "version": "1.0.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register --transpile-only server",
    "start": "npm run dev",
    "prebuild": "npm run clean && cd app && next build",
    "build": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register --transpile-only server",
    "clean": "rm -rf app/.next",
    "checkcode": "eslint -c .eslintrc.js \"**/*.{js,jsx,ts,tsx}\"",
    "type-check": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo '执行代码格式检测' && npm run checkcode"
    }
  },
  "dependencies": {
    "antd": "3.26.20",
    "axios": "^0.21.1",
    "beauty-logger": "^4.4.0",
    "express": "^4.17.1",
    "less": "^3.11.1",
    "next": "^10.2.3",
    "query-string": "^4.3.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@next/react-dev-overlay": "10.0.0",
    "@shuyun-ep-team/eslint-config": "^1.3.6",
    "@shuyun-ep-team/specified-package-version-check": "^1.3.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.4.13",
    "@types/react": "^16.14.11",
    "@types/react-dom": "^16.9.14",
    "@types/react-router-dom": "^5.1.8",
    "@types/redux-logger": "^3.0.9",
    "@types/uuid": "^8.3.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.2",
    "husky": "^4.3.8",
    "next-compose-plugins": "^2.2.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^3.9.7",
    "webpack": "^4.46.0"
  },
  "repository": "https://github.com/zhoushoujian/nextjs",
  "license": "MIT"
}
